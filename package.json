{
  "name": "online-travel-agency-system",
  "version": "0.0.0",
  "description": "",
  "author": "Sebastian Curtis T. Lavarias",
  "scripts": {
    "build:production": "concurrently \"tsc\" \"npm run build:production --prefix web-client\"",
    "build:staging": "concurrently \"tsc\" \"npm run build:staging --prefix web-client\"",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:web-client\"",
    "dev:server": "nodemon --exec ts-node src/server.ts",
    "dev:web-client": "npm run serve --prefix web-client",
    "generate:development-seeds": "ts-node src/database/seeds/index.ts",
    "generate:production-seeds": "node dist/database/seeds/index.js",
    "start": "node dist/server.js"
  },
  "main": "dist/server.js",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.25",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^8.0.29",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/validator": "^13.1.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "ts-node": "3.3.0",
    "typescript": "^4.0.2"
  },
  "bugs": {
    "url": "https://github.com/bastilavarias/online-travel-agency-system/issues"
  },
  "homepage": "https://github.com/bastilavarias/online-travel-agency-system#readme",
